e":"components/DragonAvatar.js","original":{"line":53,"column":56},"name":"join"},{"generated":{"line":126,"column":13},"source":"components/DragonAvatar.js","original":{"line":53,"column":18}},{"generated":{"line":126,"column":14},"source":"components/DragonAvatar.js","original":{"line":53,"column":61}},{"generated":{"line":126,"column":18},"source":"components/DragonAvatar.js","original":{"line":53,"column":18}},{"generated":{"line":126,"column":19},"source":"components/DragonAvatar.js","original":{"line":49,"column":12}},{"generated":{"line":126,"column":21},"source":"components/DragonAvatar.js","original":{"line":54,"column":17}},{"generated":{"line":126,"column":26},"source":"components/DragonAvatar.js","original":{"line":54,"column":22},"name":"DragonImage"},{"generated":{"line":126,"column":37},"source":"components/DragonAvatar.js","original":{"line":49,"column":12}},{"generated":{"line":126,"column":38},"source":"components/DragonAvatar.js","original":{"line":48,"column":8}},{"generated":{"line":127,"column":4},"source":"components/DragonAvatar.js","original":{"line":57,"column":5}},{"generated":{"line":131,"column":2},"source":"components/DragonAvatar.js","original":{"line":21,"column":27},"name":"Component"},{"generated":{"line":133,"column":15},"source":"components/DragonAvatar.js","original":{"line":60,"column":15},"name":"DragonAvatar"}],"sources":{"components/DragonAvatar.js":"import React, { Component } from 'react';\r\nimport {skinny, slender, patchy, plain, sporty, spotted, stocky, striped} from '../assets';\r\n\r\nconst propertyMap = {\r\n    backgroundColor: {\r\n        black: '#263238',\r\n        white: '#cfd8dc',\r\n        green: '#a5d6a7',\r\n        blue: '#0277bd',\r\n},\r\n    build: {slender, stocky, sporty, skinny},\r\n    pattern: {plain, striped, spotted, patchy},\r\n    size: {\r\n        small: 150,\r\n        medium: 250,\r\n        large: 400,\r\n        huge: 600,\r\n    }\r\n};\r\n\r\nclass DragonAvatar extends Component{\r\n\r\n    get DragonImage(){\r\n        const dragonPropertyMap = {};\r\n\r\n        this.props.dragon.traits.forEach(trait => {\r\n            const {traitType, traitValue} = trait;\r\n\r\n            dragonPropertyMap[traitType] = propertyMap[traitType][traitValue];\r\n        });\r\n\r\n        const {backgroundColor, build, pattern, size} = dragonPropertyMap;\r\n\r\n        const sizing = {width: size, height: size};\r\n\r\n        return (\r\n            <div className='dragon-avatar-image-wrapper'>\r\n                <div className='dragon-avatar-image-background' style={{backgroundColor, ...sizing }}></div>\r\n                <img src={pattern} className='dragon-avatar-image-pattern' style={{...sizing}}/>\r\n                <img src={build}className='dragon-avatar-image'style={{...sizing}}/>\r\n            </div>\r\n        );\r\n    }\r\n\r\n    render(){\r\n        const {generationId, dragonId, traits} = this.props.dragon; //props pass down from parent(dragon) to child(here)\r\n\r\n        return (\r\n            <div>\r\n                <span>Gen:{generationId}, </span>\r\n                <span>Id:{dragonId}- </span>\r\n\r\n                { traits.map(trait => trait.traitValue).join(', ') }\r\n                {this.DragonImage }\r\n            </div>\r\n        )\r\n    }\r\n}\r\n\r\nexport default DragonAvatar;"},"lineCount":null}},"error":null,"hash":"becbef27c2bf2c55242cc2cc354f81d2","cacheData":{"env":{}}}